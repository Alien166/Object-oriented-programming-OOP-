#include<bits/stdc++.h>
#include<conio.h>
using namespace std;
class vehicle
{
public:
    vehicle()
    {
        cout<<"This is a vehicle"<<endl;
    }
};
class FourWheeler
{
public:
    FourWheeler()
    {
        cout<<"This is a four wheeler"<<endl;
    }
};
class car:public vehicle,public FourWheeler{};
int main()
{
    car obj;
    getch();
    return 0;
}
