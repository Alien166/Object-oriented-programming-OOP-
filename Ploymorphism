#include<bits/stdc++.h>
using namespace std;
class geeks
{
public:
    void func(int x)
    {
        cout<<"Value of x is "<<x<<endl;
    }
    void func(double y)
    {
        cout<<"value of y is "<<y<<endl;
    }
    void func(int x,double y)
    {
        cout<<"value of x and y "<<x<<" " <<y<<endl;
    }

};

int main()
{
geeks obj;
obj.func(6);
obj.func(3.12);
obj.func(23,2.23);
    return 0;
}
